{
    "name": "decodelabs/systemic",
    "description": "Easy access to system and environment info",
    "type": "library",
    "keywords": ["system", "environment"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "symfony/polyfill-mbstring": "^1.7",

        "decodelabs/coercion": "^0.2",
        "decodelabs/deliverance": "^0.2",
        "decodelabs/exceptional": "^0.4",
        "decodelabs/fluidity": "^0.3",
        "decodelabs/glitch-support": "^0.4",
        "decodelabs/veneer": "^0.10.10"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.6",
        "decodelabs/terminus": "^0.9.0|dev-develop"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Systemic\\": "src/Systemic"
        },
        "files": [
            "src/global.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.10.x-dev"
        }
    }
}
